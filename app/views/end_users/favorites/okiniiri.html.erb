<h1><%= current_end_user.name %>さんのお気に入りページ</h1>


<% current_end_user.favorites.each do |f| %>
<%= f.product.product_name %>
<%= link_to end_users_product_path(f.product) do %>
<div><%= attachment_image_tag f.product, :product_image, :fill, 200, 200, fallback: "no_image.jpg", size: '200x200' %><% end %><div>
<% end %>

